{% extends 'baseback.html.twig' %}

{% block title %}New Categorie{% endblock %}

{% block body %}
<div class="row">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Category form</h4>
                <p class="card-description">Add Category</p>
                {{ form_start(form) }}
